<meta:Type
  xmlns:meta="class:/spiralcraft/data/types/meta/"
  >
  
  <fields>

    <meta:Field>
      <name>streetAddress
      </name>
      <type><String.type/>
      </type>
      <description>Building number, street name
      </description>
    </meta:Field>

    <meta:Field>
      <name>postOfficeBox</name>
      <type><String.type/>
      </type>
      <description>example: PO BOX 123
      </description>
    </meta:Field>

    <meta:Field>
      <name>locator
      </name>
      <type><String.type/>
      </type>
      <description>Unit, Bldg., Apt., Ste., Mail Stop, etc. 
      </description>
    </meta:Field>
    
    <meta:Field>
      <name>locality</name>
      <type><String.type/>
      </type>
      <description>City or other locality
      </description>
    </meta:Field>
    
    <meta:Field>
      <name>region</name>
      <type><String.type/>
      </type>
      <description>State, province or territory
      </description>
    </meta:Field>
    
    <meta:Field>
      <name>postalCode</name>
      <type><String.type/>
      </type>
    </meta:Field>
    
    <meta:Field>
      <name>countryCode</name>
      <type><String.type/>
      </type>
    </meta:Field>    
  </fields>
  
</meta:Type>